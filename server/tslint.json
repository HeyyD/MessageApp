{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "ordered-imports": false,
    "no-console": false,
    "member-access": false,
    "semicolon": true,
    "object-literal-sort-keys": false,
    "interface-name": [true, "never-prefix"],
    "no-implicit-dependencies": [true, "dev"]
  }
}
